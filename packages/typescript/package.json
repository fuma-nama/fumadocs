{
  "name": "fumadocs-typescript",
  "version": "5.1.4",
  "description": "Typescript Integration for Fumadocs",
  "keywords": [
    "Docs",
    "NextJs",
    "fumadocs"
  ],
  "homepage": "https://fumadocs.dev",
  "license": "MIT",
  "author": "Fuma Nama",
  "repository": "github:fuma-nama/fumadocs",
  "files": [
    "dist/*"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./ui": {
      "types": "./dist/ui/index.d.ts",
      "import": "./dist/ui/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "clean": "rimraf dist",
    "dev": "tsdown --watch",
    "lint": "eslint .",
    "types:check": "tsc --noEmit"
  },
  "dependencies": {
    "estree-util-value-to-estree": "^3.5.0",
    "hast-util-to-estree": "^3.1.3",
    "hast-util-to-jsx-runtime": "^2.3.6",
    "remark": "^15.0.1",
    "remark-rehype": "^11.1.2",
    "ts-morph": "^27.0.2",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.1.0"
  },
  "devDependencies": {
    "@mdx-js/mdx": "^3.1.1",
    "@types/estree": "^1.0.8",
    "@types/hast": "^3.0.4",
    "@types/mdast": "^4.0.4",
    "@types/node": "25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "eslint-config-custom": "workspace:*",
    "fumadocs-core": "workspace:*",
    "fumadocs-ui": "workspace:*",
    "mdast-util-mdx": "^3.0.0",
    "tsconfig": "workspace:*",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3",
    "vfile": "^6.0.3"
  },
  "peerDependencies": {
    "@types/estree": "*",
    "@types/hast": "*",
    "@types/mdast": "*",
    "@types/react": "*",
    "fumadocs-core": "^16.5.0",
    "fumadocs-ui": "^16.5.0",
    "react": "*",
    "typescript": "*"
  },
  "peerDependenciesMeta": {
    "@types/estree": {
      "optional": true
    },
    "@types/hast": {
      "optional": true
    },
    "@types/mdast": {
      "optional": true
    },
    "fumadocs-ui": {
      "optional": true
    },
    "@types/react": {
      "optional": true
    }
  }
}
