{
  "name": "next-docs-ui",
  "version": "6.0.2",
  "description": "The framework for building a documentation website in Next.js",
  "keywords": [
    "NextJs",
    "Docs"
  ],
  "homepage": "https://next-docs-zeta.vercel.app",
  "repository": "github:fuma-nama/next-docs",
  "license": "MIT",
  "author": "Fuma Nama",
  "exports": {
    "./style.css": "./dist/style.css",
    "./tailwind-plugin": {
      "import": "./dist/tailwind-plugin.js",
      "require": "./dist/tailwind-plugin.js",
      "types": "./dist/tailwind-plugin.d.ts"
    },
    "./components/*": {
      "import": "./dist/components/*.js",
      "types": "./dist/components/*.d.mts"
    },
    "./mdx/*": {
      "import": "./dist/mdx/*.js",
      "types": "./dist/mdx/*.d.mts"
    },
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.mts"
    }
  },
  "typesVersions": {
    "*": {
      "tailwind-plugin": [
        "./dist/tailwind-plugin.d.ts"
      ],
      "components/*": [
        "./dist/components/*.d.mts"
      ],
      "mdx/*": [
        "./dist/mdx/*.d.mts"
      ],
      "*": [
        "./dist/*.d.mts"
      ]
    }
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsup && postcss css/styles.css -o ./dist/style.css",
    "clean": "rmdir /q/s dist",
    "dev": "concurrently \"pnpm dev:layout\" \"pnpm dev:tailwind\"",
    "dev:layout": "tsup --watch",
    "dev:tailwind": "postcss css/styles.css -o ./dist/style.css --watch",
    "lint": "eslint .",
    "types:check": "tsc --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-scroll-area": "^1.0.4",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-tabs": "^1.0.4",
    "@tailwindcss/typography": "^0.5.10",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "lucide-react": "^0.297.0",
    "next-docs-zeta": "workspace:*",
    "next-themes": "^0.2.1",
    "react-medium-image-zoom": "^5.1.8",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@algolia/client-search": "^4.20.0",
    "@next/eslint-plugin-next": "^14.0.0",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.1",
    "algoliasearch": "^4.20.0",
    "eslint-config-custom": "workspace:*",
    "next": "14.0.4",
    "postcss": "8.4.32",
    "postcss-cli": "^11.0.0",
    "postcss-lightningcss": "^1.0.0",
    "tailwindcss": "3.3.6",
    "tsconfig": "workspace:*"
  },
  "peerDependencies": {
    "next": ">= 13",
    "react": ">= 18",
    "react-dom": ">= 18"
  },
  "publishConfig": {
    "access": "public"
  }
}
