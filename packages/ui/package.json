{
  "name": "@fumadocs/ui-utils",
  "version": "16.2.5",
  "description": "The React.js documentation framework",
  "keywords": [
    "Fumadocs",
    "Docs"
  ],
  "homepage": "https://fumadocs.dev",
  "repository": "github:fuma-nama/fumadocs",
  "license": "MIT",
  "author": "Fuma Nama",
  "type": "module",
  "exports": {
    "./css/*": "./css/*",
    "./i18n": {
      "import": "./dist/i18n.js",
      "types": "./dist/i18n.d.ts"
    },
    "./og/*": {
      "import": "./dist/og/*.js",
      "types": "./dist/og/*.d.ts"
    },
    "./contexts/*": {
      "import": "./dist/contexts/*.js",
      "types": "./dist/contexts/*.d.ts"
    },
    "./theme/*": {
      "import": "./dist/theme/*.js",
      "types": "./dist/theme/*.d.ts"
    },
    "./utils/*": {
      "import": "./dist/utils/*.js",
      "types": "./dist/utils/*.d.ts"
    },
    "./icons": {
      "import": "./dist/icons.js",
      "types": "./dist/icons.d.ts"
    }
  },
  "files": [
    "dist/*",
    "css/*"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "clean": "rimraf dist",
    "dev": "concurrently \"tsc -w --project tsconfig.build.json\" \"tsc-alias -w -p tsconfig.build.json\"",
    "lint": "eslint .",
    "types:check": "tsc --noEmit"
  },
  "dependencies": {
    "fumadocs-core": "workspace:*",
    "lodash.merge": "^4.6.2",
    "next-themes": "^0.4.6",
    "postcss-selector-parser": "^7.1.1",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@fumadocs/cli": "workspace:*",
    "@types/lodash.merge": "^4.6.9",
    "@types/node": "^24.10.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "eslint-config-custom": "workspace:*",
    "fumadocs-core": "workspace:*",
    "next": "16.0.10",
    "tailwindcss": "^4.1.18",
    "tsc-alias": "^1.8.16",
    "tsconfig": "workspace:*"
  },
  "peerDependencies": {
    "@types/react": "*",
    "next": "16.x.x",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    },
    "@types/react": {
      "optional": true
    },
    "tailwindcss": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
