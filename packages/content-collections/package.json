{
  "name": "@fumadocs/content-collections",
  "version": "1.2.5",
  "description": "The Content Collections adapter for Fumadocs",
  "keywords": [
    "Content Collections",
    "Docs",
    "NextJs"
  ],
  "homepage": "https://fumadocs.dev",
  "license": "MIT",
  "author": "Fuma Nama",
  "repository": "github:fuma-nama/fumadocs",
  "files": [
    "dist/*"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./configuration": {
      "types": "./dist/configuration.d.ts",
      "import": "./dist/configuration.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "clean": "rimraf dist",
    "dev": "tsdown --watch",
    "lint": "eslint .",
    "types:check": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@content-collections/core": "^0.13.0",
    "@content-collections/mdx": "^0.2.2",
    "@types/node": "24.10.2",
    "eslint-config-custom": "workspace:*",
    "fumadocs-core": "workspace:*",
    "tsconfig": "workspace:*",
    "tsdown": "^0.18.4",
    "unified": "^11.0.5"
  },
  "peerDependencies": {
    "@content-collections/core": "0.x.x",
    "@content-collections/mdx": "0.x.x",
    "fumadocs-core": "15.x.x || 16.x.x"
  }
}
