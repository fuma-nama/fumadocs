{
  "name": "next-docs-mdx",
  "description": "The library for using native Next.js MDX features in Next Docs",
  "version": "5.0.0",
  "homepage": "https://next-docs-zeta.vercel.app",
  "author": "Fuma Nama",
  "repository": "github:fuma-nama/next-docs",
  "license": "MIT",
  "files": [
    "dist/*",
    "loader.js"
  ],
  "exports": {
    "./loader": "./loader.js",
    "./loader-mdx": "./loader-mdx.js",
    "./config": {
      "import": "./dist/config.mjs",
      "types": "./dist/config.d.mts"
    },
    "./map": {
      "import": "./dist/map.mjs",
      "types": "./dist/map.d.mts"
    }
  },
  "typesVersions": {
    "*": {
      "config": [
        "./dist/config.d.mts"
      ],
      "map": [
        "./dist/map.d.mts"
      ],
      "types": [
        "./dist/types.d.mts"
      ]
    }
  },
  "scripts": {
    "build": "tsup",
    "clean": "rmdir /q/s dist",
    "dev": "tsup --watch",
    "prepublishOnly": "pnpm build",
    "types:check": "tsc --noEmit"
  },
  "dependencies": {
    "@mdx-js/mdx": "^3.0.0",
    "estree-util-value-to-estree": "^3.0.1",
    "fast-glob": "^3.3.1",
    "gray-matter": "^4.0.3",
    "next-docs-zeta": "workspace:*",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "next": ">= 13.4"
  },
  "devDependencies": {
    "@types/mdast": "^4.0.3",
    "@types/mdx": "^2.0.8",
    "@types/react": "18.2.0",
    "next": "14.0.4",
    "unified": "^11.0.4",
    "webpack": "^5.89.0"
  },
  "keywords": [
    "NextJs",
    "Docs"
  ],
  "publishConfig": {
    "access": "public"
  }
}
