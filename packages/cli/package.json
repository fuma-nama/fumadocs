{
  "name": "@fumadocs/cli",
  "version": "1.2.1",
  "description": "The CLI tool for Fumadocs",
  "keywords": [
    "Docs",
    "Fumadocs"
  ],
  "homepage": "https://fumadocs.dev",
  "license": "MIT",
  "author": "Fuma Nama",
  "repository": "github:fuma-nama/fumadocs",
  "bin": {
    "fumadocs": "./dist/index.js"
  },
  "files": [
    "dist/*"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    "./build": {
      "types": "./dist/build/index.d.ts",
      "import": "./dist/build/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown --config-loader unrun",
    "clean": "rimraf dist",
    "dev": "tsdown --watch --config-loader unrun",
    "lint": "eslint .",
    "types:check": "tsc --noEmit"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "commander": "^14.0.2",
    "package-manager-detector": "^1.6.0",
    "picocolors": "^1.1.1",
    "tinyexec": "^1.0.2",
    "ts-morph": "^27.0.2",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/node": "24.10.2",
    "eslint-config-custom": "workspace:*",
    "shadcn": "3.6.2",
    "tsconfig": "workspace:*",
    "tsdown": "^0.18.3"
  }
}
