{
  "name": "@fumadocs/cli",
  "version": "1.0.3",
  "description": "The CLI tool for Fumadocs",
  "keywords": [
    "NextJs",
    "Docs",
    "Fumadocs"
  ],
  "homepage": "https://fumadocs.dev",
  "repository": "github:fuma-nama/fumadocs",
  "license": "MIT",
  "author": "Fuma Nama",
  "type": "module",
  "exports": {
    "./build": {
      "import": "./dist/build/index.js",
      "types": "./dist/build/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "bin": {
    "fumadocs": "./dist/index.js"
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "types:check": "tsc --noEmit"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "commander": "^14.0.2",
    "package-manager-detector": "^1.5.0",
    "picocolors": "^1.1.1",
    "tinyexec": "^1.0.2",
    "ts-morph": "^27.0.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "24.10.1",
    "eslint-config-custom": "workspace:*",
    "shadcn": "3.5.0",
    "tsconfig": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  }
}
