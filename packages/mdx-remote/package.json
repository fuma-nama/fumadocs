{
  "name": "@fumadocs/mdx-remote",
  "version": "0.2.4",
  "description": "The remote MDX files adapter for Fumadocs",
  "keywords": [
    "NextJs",
    "Docs",
    "fumadocs",
    "next-mdx-remote"
  ],
  "homepage": "https://fumadocs.vercel.app",
  "repository": "github:fuma-nama/fumadocs",
  "license": "MIT",
  "author": "Fuma Nama",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "types:check": "tsc --noEmit"
  },
  "dependencies": {
    "@mdx-js/mdx": "^3.1.0",
    "gray-matter": "^4.0.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "fast-glob": "^3.3.3",
    "@types/mdx": "^2.0.13",
    "@types/node": "22.10.6",
    "@types/react": "^19.0.7",
    "eslint-config-custom": "workspace:*",
    "fumadocs-core": "workspace:*",
    "next": "15.1.4",
    "tsconfig": "workspace:*",
    "unified": "^11.0.5",
    "vfile": "^6.0.3"
  },
  "peerDependencies": {
    "fumadocs-core": "^13.2.1 || ^14.0.0",
    "next": "14.x.x || 15.x.x",
    "react": "18.x.x || 19.x.x"
  },
  "publishConfig": {
    "access": "public"
  }
}
