{
  "name": "fumapress",
  "version": "0.0.8",
  "description": "The minimal setup for Fumadocs",
  "keywords": [
    "Docs",
    "Fumadocs"
  ],
  "homepage": "https://fumadocs.dev",
  "license": "MIT",
  "author": "Fuma Nama",
  "repository": "github:fuma-nama/fumadocs",
  "bin": {
    "fumapress": "./dist/cli/index.mjs"
  },
  "files": [
    "dist/*"
  ],
  "type": "module",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "types": "./dist/vite/index.d.mts",
      "import": "./dist/vite/index.mjs"
    },
    "./*": {
      "types": "./dist/*.d.mts",
      "import": "./dist/*.mjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "types:check": "tsc --noEmit"
  },
  "dependencies": {
    "@mdx-js/mdx": "^3.1.1",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitejs/plugin-rsc": "^0.5.16",
    "commander": "^14.0.2",
    "fumadocs-core": "workspace:*",
    "fumadocs-mdx": "workspace:*",
    "fumadocs-ui": "workspace:*",
    "picocolors": "^1.1.1",
    "react-router": "^7.13.0",
    "tinyglobby": "^0.2.15",
    "vite": "^7.3.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/mdx": "^2.0.13",
    "@types/node": "^25.0.10",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "eslint-config-custom": "workspace:*",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tsconfig": "workspace:*",
    "tsdown": "^0.19.0",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "peerDependenciesMeta": {
    "react-dom": {
      "optional": true
    }
  }
}
