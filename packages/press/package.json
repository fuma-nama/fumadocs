{
  "name": "fumapress",
  "version": "0.0.1",
  "description": "The minimal setup for Fumadocs",
  "keywords": [
    "Fumadocs",
    "Docs"
  ],
  "homepage": "https://fumadocs.dev",
  "repository": "github:fuma-nama/fumadocs",
  "license": "MIT",
  "author": "Fuma Nama",
  "type": "module",
  "files": [
    "src/*",
    "dist/*"
  ],
  "bin": {
    "fumapress": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@vitejs/plugin-react": "^5.0.3",
    "@vitejs/plugin-rsc": "^0.4.32",
    "commander": "^14.0.1",
    "fumadocs-core": "workspace:*",
    "fumadocs-mdx": "workspace:*",
    "fumadocs-ui": "workspace:*",
    "picocolors": "^1.1.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.9.3",
    "tinyglobby": "^0.2.15",
    "vite": "^7.1.7"
  },
  "devDependencies": {
    "@types/mdx": "^2.0.13",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.14",
    "@types/react-dom": "^19.1.9",
    "eslint-config-custom": "workspace:*",
    "tsconfig": "workspace:*",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "@types/react": "*"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
