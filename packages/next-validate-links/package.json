{
  "name": "next-validate-link",
  "version": "1.0.1",
  "description": "An utility to validate links in markdown file",
  "keywords": [
    "NextJs",
    "Docs",
    "fumadocs",
    "markdown"
  ],
  "homepage": "https://fumadocs.vercel.app",
  "repository": "github:fuma-nama/fumadocs",
  "license": "MIT",
  "author": "Fuma Nama",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "types:check": "tsc --noEmit"
  },
  "dependencies": {
    "fast-glob": "^3.3.1",
    "gray-matter": "^4.0.3",
    "picocolors": "^1.1.1",
    "remark": "^15.0.1",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "22.8.1",
    "eslint-config-custom": "workspace:*",
    "tsconfig": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  }
}
